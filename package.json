{
  "name": "@arwed/homepage-core",
  "version": "0.0.2",
  "description": "My Homepage Core Component",
  "main": "build/index.js",
  "scripts": {
    "test": "npm run lint && npm run flow && npm run mocha",
    "mocha": "mocha --require babel-core/register --require babel-polyfill --slow 30 --throw-deprecation --use_strict",
    "mocha-watch": "npm run mocha -- --watch --watch-extensions js",
    "clean": "rm -rf build",
    "flow": "flow",
    "lint": "eslint src test",
	"prepack": "npm run clean && npm run test && npm run prepack:babel && npm run prepack:flow",
	"prepack:babel": "babel src --out-dir build",
	"prepack:flow": "flow-copy-source src build"
  },
  "author": "Arwed Mett",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-mocha": "^5.0.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.69.0",
    "flow-copy-source": "^2.0.2",
    "flow-typed": "^2.4.0",
    "mocha": "^5.1.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-3": "^6.24.1",
    "chalk": "^2.3.2",
    "node-fetch": "^2.2.0",
    "sequelize": "^4.38.1",
    "sqlite3": "^4.0.2"
  }
}
