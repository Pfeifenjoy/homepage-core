{
  "name": "@arwed/homepage-core",
  "version": "0.0.21",
  "description": "My Homepage Core Component",
  "main": "build/index.js",
  "scripts": {
    "test": "npm run lint && npm run flow && npm run mocha",
    "mocha": "mocha --require @babel/register --require @babel/polyfill --use_strict",
    "mocha-watch": "npm run mocha -- --watch --watch-extensions js",
    "clean": "rm -rf build",
    "flow": "flow-copy-source src build",
    "lint": "eslint src test",
    "build": "npm run clean && npm run babel && npm run flow",
    "prepack": "npm run test && npm run build",
    "babel": "babel src --out-dir build"
  },
  "author": "Arwed Mett",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-mocha": "^5.0.0",
    "flow-bin": "^0.89.0",
    "flow-copy-source": "^2.0.2",
    "mocha": "^5.1.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0"
  },
  "dependencies": {
    "@arwed/logging": "0.0.0",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.2",
    "node-fetch": "^2.2.0",
    "pem": "^1.13.2",
    "rmrf": "^2.0.0",
    "sequelize": "^4.42.0",
    "sqlite3": "^4.0.4"
  }
}
